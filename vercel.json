{
    "version": 2,
    "name": "universal-media-downloader",
    "rewrites": [
        {
            "source": "/api/download",
            "destination": "/api/info.py"
        },
        {
            "source": "/api/proxy-download",
            "destination": "/api/stream.py"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/$1"
        }
    ]
}